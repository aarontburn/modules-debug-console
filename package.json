{
  "name": "nexus-debug-console",
  "version": "1.0.0",
  "description": "A debug console module for Nexus.",
  "author": "aarontburn",
  "config": {
    "react-wrapper-directory": "./src/renderer/react-wrapper"
  },
  "scripts": {
    "preinstall": "cd %npm_package_config_react-wrapper-directory% && npm install",
    "build": "npm run dev_export",
    "start": "npm-run-all --parallel vite:start electron-start",
    "export": "npm run vite:build && node node_modules/@nexus/nexus-exporter/exporter.js",
    "dev_export": "npm run vite:build && node node_modules/@nexus/nexus-exporter/exporter.js --dev",
    "vite:build": "cd %npm_package_config_react-wrapper-directory% && npm run build",
    "vite:start": "cd %npm_package_config_react-wrapper-directory% && npm run dev",
    "electron-start": "npm run build && electron ./node_modules/@nexus/nexus-client/main.js --dev",
    "update-nexus": "npm update @nexus/nexus-exporter && npm update @nexus/nexus-module-builder && npm update @nexus/nexus-client"
  },
  "devDependencies": {
    "@nexus/nexus-client": "https://gitpkg.vercel.app/aarontburn/nexus-core/nexus/dist?main",
    "@nexus/nexus-exporter": "https://gitpkg.vercel.app/aarontburn/nexus-core/exporter?main",
    "@nexus/nexus-module-builder": "https://gitpkg.vercel.app/aarontburn/nexus-core/module-builder/dist?main",
    "@types/callsite": "^1.0.34",
    "@types/yauzl-promise": "^4.0.1",
    "electron": "^28.0.0",
    "npm-run-all": "^4.1.5",
    "typescript": "^4.8.3",
    "yauzl-promise": "^4.0.0"
  },
  "dependencies": {
    "callsite": "^1.0.0"
  }
}
